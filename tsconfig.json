{
	"compilerOptions": {
		"module": "commonjs",
		"moduleResolution": "Node16",
		"noImplicitAny": true,
		"preserveConstEnums": true,
		"sourceMap": true,
		"strict": true,
		"downlevelIteration": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"outDir": "lib",
		"rootDir": "src"
	},
	"include": ["./src/**/*", "./tsconfig.json"]
}
